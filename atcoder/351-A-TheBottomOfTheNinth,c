#include <stdio.h>
int main(void)
{
    int a = 0;
    int b = 0;
    int tmp;
    int scan;

    for(int i=0;i<9;i++) {
        scan = scanf("%d",&tmp);
        a += tmp; 
    }    
    for(int i=0;i<8;i++) {
        scan = scanf("%d",&tmp);
        b += tmp; 
    }    
    printf("%d\n",a-b+1);
    return 0;
}